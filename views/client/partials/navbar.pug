nav.tyn-appbar
  .tyn-appbar-wrap
    .tyn-appbar-logo
      a.tyn-logo(href='/messages')
        svg(viewbox='0 0 43 40' fill='none' xmlns='http://www.w3.org/2000/svg')
          path(d='M37.2654 14.793C37.2654 14.793 45.0771 20.3653 41.9525 29.5311C41.9525 29.5311 41.3796 31.1976 39.0361 34.4264L42.4732 37.9677C42.4732 37.9677 43.3065 39.478 41.5879 39.9987H24.9229C24.9229 39.9987 19.611 40.155 14.8198 36.9782C14.8198 36.9782 12.1638 35.2076 9.76825 31.9787L18.6215 32.0308C18.6215 32.0308 24.298 31.9787 29.7662 28.3333C35.2344 24.6878 37.4217 18.6988 37.2654 14.793Z' fill='#60A5FA')
          path(d='M34.5053 12.814C32.2659 1.04441 19.3506 0.0549276 19.3506 0.0549276C8.31004 -0.674164 3.31055 6.09597 3.31055 6.09597C-4.24076 15.2617 3.6751 23.6983 3.6751 23.6983C3.6751 23.6983 2.99808 24.6357 0.862884 26.5105C-1.27231 28.3854 1.22743 29.3748 1.22743 29.3748H17.3404C23.4543 28.7499 25.9124 27.3959 25.9124 27.3959C36.328 22.0318 34.5053 12.814 34.5053 12.814ZM19.9963 18.7301H9.16412C8.41419 18.7301 7.81009 18.126 7.81009 17.3761C7.81009 16.6261 8.41419 16.022 9.16412 16.022H19.9963C20.7463 16.022 21.3504 16.6261 21.3504 17.3761C21.3504 18.126 20.7358 18.7301 19.9963 18.7301ZM25.3708 13.314H9.12245C8.37253 13.314 7.76843 12.7099 7.76843 11.96C7.76843 11.21 8.37253 10.6059 9.12245 10.6059H25.3708C26.1207 10.6059 26.7248 11.21 26.7248 11.96C26.7248 12.7099 26.1103 13.314 25.3708 13.314Z' fill='#2563EB')
    // tyn-logo
    .tyn-appbar-content
      ul.tyn-appbar-nav.tyn-appbar-nav-start
        li.tyn-appbar-item(class=`${activeTab === 'messages' ? 'active current-page' : ''}`)
          a.tyn-appbar-link(href='/messages')
            // chat-text-fill
            svg.bi.bi-chat-text-fill(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
              path(d='M16 8c0 3.866-3.582 7-8 7a9 9 0 0 1-2.347-.306c-.584.296-1.925.864-4.181 1.234-.2.032-.352-.176-.273-.362.354-.836.674-1.95.77-2.966C.744 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7M4.5 5a.5.5 0 0 0 0 1h7a.5.5 0 0 0 0-1zm0 2.5a.5.5 0 0 0 0 1h7a.5.5 0 0 0 0-1zm0 2.5a.5.5 0 0 0 0 1h4a.5.5 0 0 0 0-1z')
            span.d-none Chats
        // .tyn-appbar-item
        li.tyn-appbar-item(class=`${activeTab === 'contacts' ? 'active current-page' : ''}`)
          a.tyn-appbar-link(href='/contacts')
            // person-lines-fill
            svg.bi.bi-person-lines-fill(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
              path(d='M6 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6m-5 6s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1zM11 3.5a.5.5 0 0 1 .5-.5h4a.5.5 0 0 1 0 1h-4a.5.5 0 0 1-.5-.5m.5 2.5a.5.5 0 0 0 0 1h4a.5.5 0 0 0 0-1zm2 3a.5.5 0 0 0 0 1h2a.5.5 0 0 0 0-1zm0 3a.5.5 0 0 0 0 1h2a.5.5 0 0 0 0-1z')
            span.d-none Contacts
        // .tyn-appbar-item
      // .tyn-appbar-nav
      ul.tyn-appbar-nav.tyn-appbar-nav-end
        li.tyn-appbar-item(notifications)
          a.tyn-appbar-link.dropdown-toggle.position-relative(data-bs-toggle='dropdown' href='#' data-bs-offset='0,10' data-bs-auto-close='outside')
            // app-indicator
            svg.bi.bi-app-indicator(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
              path(d='M5.5 2A3.5 3.5 0 0 0 2 5.5v5A3.5 3.5 0 0 0 5.5 14h5a3.5 3.5 0 0 0 3.5-3.5V8a.5.5 0 0 1 1 0v2.5a4.5 4.5 0 0 1-4.5 4.5h-5A4.5 4.5 0 0 1 1 10.5v-5A4.5 4.5 0 0 1 5.5 1H8a.5.5 0 0 1 0 1z')
              path(d='M16 3a3 3 0 1 1-6 0 3 3 0 0 1 6 0')
            .count-request.badge.bg-danger
              | #{user.contactRequestsReceived.length}
          .dropdown-menu.dropdown-menu-rg.dropdown-menu-end
            .dropdown-head
              .title
                h6 Notifications
              ul.nav.nav-tabs.nav-tabs-line
                li.nav-item
                  button.nav-link.active(data-bs-toggle='tab' data-bs-target='#notifications-all' type='button')  All 
            // .dropdown-head
            .dropdown-gap
              ul.tyn-media-list.gap.gap-3.request-list
                each requestContact in user.contactRequestsReceived
                  li
                    .tyn-media-group.align-items-start
                      .tyn-media.tyn-circle
                        img(src=requestContact.avatar alt=requestContact.fullName)
                      .tyn-media-col
                        .tyn-media-row
                          span.message
                            strong #{requestContact.fullName}
                            |  Added You
                        .tyn-media-row
                          ul.tyn-btn-inline.gap.gap-3.pt-1
                            li
                              button.btn.btn-md.btn-primary(accept-request data-username=requestContact.username)
                                // check2-circle
                                svg.bi.bi-check2-circle(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                                  path(d='M2.5 8a5.5 5.5 0 0 1 8.25-4.764.5.5 0 0 0 .5-.866A6.5 6.5 0 1 0 14.5 8a.5.5 0 0 0-1 0 5.5 5.5 0 1 1-11 0')
                                  path(d='M15.354 3.354a.5.5 0 0 0-.708-.708L8 9.293 5.354 6.646a.5.5 0 1 0-.708.708l3 3a.5.5 0 0 0 .708 0z')
                                span Accept
                            li
                              button.btn.btn-md.btn-light(reject-request data-username=requestContact.username)
                                // x-circle
                                svg.bi.bi-x-circle(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                                  path(d='M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14m0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16')
                                  path(d='M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708')
                                span Reject
                    // .tyn-media-group
                  // li
                else
                  li.no-noti
                    span.message
                      | No new notifications
              // .tyn-media-list
            // .dropdown-gap
          // .dropdown-menu
        // .tyn-appbar-item
        li.tyn-appbar-item
          a.d-inline-flex.dropdown-toggle(data-bs-auto-close='outside' data-bs-toggle='dropdown' href='#' data-bs-offset='0,10')
            .tyn-media.tyn-size-lg.tyn-circle
              img(src=user.avatar alt='')
          .dropdown-menu.dropdown-menu-end
            .dropdown-gap
              .tyn-media-group
                .tyn-media.tyn-size-lg
                  img(src=user.avatar alt='')
                .tyn-media-col
                  .tyn-media-row
                    h6.name #{user.fullName}
                    .indicator.varified
                      // check-circle-fill
                      svg.bi.bi-check-circle-fill(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                        path(d='M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0m-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z')
                  .tyn-media-row.has-dot-sap
                    p.content #{user.bio ? user.bio : 'No bio'}
            // .dropdown-gap
            .dropdown-gap
              .d-flex.gap.gap-2
                // moon-fill
                svg.bi.bi-moon-fill(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                  path(d='M6 .278a.77.77 0 0 1 .08.858 7.2 7.2 0 0 0-.878 3.46c0 4.021 3.278 7.277 7.318 7.277q.792-.001 1.533-.16a.79.79 0 0 1 .81.316.73.73 0 0 1-.031.893A8.35 8.35 0 0 1 8.344 16C3.734 16 0 12.286 0 7.71 0 4.266 2.114 1.312 5.124.06A.75.75 0 0 1 6 .278')
                div
                  h6 Darkmode
                  ul.d-flex.align-items-center.gap.gap-3
                    li.inline-flex
                      .form-check
                        input#dark.form-check-input(type='radio' name='themeMode' value='dark')
                        label.form-check-label.small(for='dark')  On 
                    li.inline-flex
                      .form-check
                        input#light.form-check-input(type='radio' name='themeMode' value='light' checked='')
                        label.form-check-label.small(for='light')  Off 
            ul.tyn-list-links
              li
                a(href='/user/profile#profile-edit')
                  // person
                  svg.bi.bi-person(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                    path(d='M8 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6m2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0m4 8c0 1-1 1-1 1H3s-1 0-1-1 1-4 6-4 6 3 6 4m-1-.004c-.001-.246-.154-.986-.832-1.664C11.516 10.68 10.289 10 8 10s-3.516.68-4.168 1.332c-.678.678-.83 1.418-.832 1.664z')
                  span Profile
              li
                a(href='/user/profile#change-password')
                  // unlock
                  svg.bi.bi-unlock(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                    path(d='M11 1a2 2 0 0 0-2 2v4a2 2 0 0 1 2 2v5a2 2 0 0 1-2 2H3a2 2 0 0 1-2-2V9a2 2 0 0 1 2-2h5V3a3 3 0 0 1 6 0v4a.5.5 0 0 1-1 0V3a2 2 0 0 0-2-2M3 8a1 1 0 0 0-1 1v5a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V9a1 1 0 0 0-1-1z')
                  span Change Password
              li
                a(href='/user/profile#profile-settings')
                  // gear
                  svg.bi.bi-gear(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                    path(d='M8 4.754a3.246 3.246 0 1 0 0 6.492 3.246 3.246 0 0 0 0-6.492M5.754 8a2.246 2.246 0 1 1 4.492 0 2.246 2.246 0 0 1-4.492 0')
                    path(d='M9.796 1.343c-.527-1.79-3.065-1.79-3.592 0l-.094.319a.873.873 0 0 1-1.255.52l-.292-.16c-1.64-.892-3.433.902-2.54 2.541l.159.292a.873.873 0 0 1-.52 1.255l-.319.094c-1.79.527-1.79 3.065 0 3.592l.319.094a.873.873 0 0 1 .52 1.255l-.16.292c-.892 1.64.901 3.434 2.541 2.54l.292-.159a.873.873 0 0 1 1.255.52l.094.319c.527 1.79 3.065 1.79 3.592 0l.094-.319a.873.873 0 0 1 1.255-.52l.292.16c1.64.893 3.434-.902 2.54-2.541l-.159-.292a.873.873 0 0 1 .52-1.255l.319-.094c1.79-.527 1.79-3.065 0-3.592l-.319-.094a.873.873 0 0 1-.52-1.255l.16-.292c.893-1.64-.902-3.433-2.541-2.54l-.292.159a.873.873 0 0 1-1.255-.52zm-2.633.283c.246-.835 1.428-.835 1.674 0l.094.319a1.873 1.873 0 0 0 2.693 1.115l.291-.16c.764-.415 1.6.42 1.184 1.185l-.159.292a1.873 1.873 0 0 0 1.116 2.692l.318.094c.835.246.835 1.428 0 1.674l-.319.094a1.873 1.873 0 0 0-1.115 2.693l.16.291c.415.764-.42 1.6-1.185 1.184l-.291-.159a1.873 1.873 0 0 0-2.693 1.116l-.094.318c-.246.835-1.428.835-1.674 0l-.094-.319a1.873 1.873 0 0 0-2.692-1.115l-.292.16c-.764.415-1.6-.42-1.184-1.185l.159-.291A1.873 1.873 0 0 0 1.945 8.93l-.319-.094c-.835-.246-.835-1.428 0-1.674l.319-.094A1.873 1.873 0 0 0 3.06 4.377l-.16-.292c-.415-.764.42-1.6 1.185-1.184l.292.159a1.873 1.873 0 0 0 2.692-1.115z')
                  span Settings
              li.dropdown-divider
              li
                a(href='/auth/logout')
                  // power
                  svg.bi.bi-power(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                    path(d='M7.5 1v7h1V1z')
                    path(d='M3 8.812a5 5 0 0 1 2.578-4.375l-.485-.874A6 6 0 1 0 11 3.616l-.501.865A5 5 0 1 1 3 8.812')
                  span Log Out
            // .tyn-list-links
          // .dropdown-menu
        // .tyn-appbar-item
      // .tyn-appbar-nav
    // .tyn-appbar-content
  // .tyn-appbar-wrap